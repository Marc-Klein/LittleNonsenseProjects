{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(/.*)*",
    "^@mui/(.*)$",
    "^@emotion/(.*)$",
    "^@fontawesome/(.*)$",
    "^@fortawesome/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[@/]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 100,
  "tabWidth": 2,
  "endOfLine": "auto",
  "trailingComma": "none",
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid"
}
